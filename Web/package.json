{
  "name": "react-webpack-typescript-starter",
  "version": "0.1.0",
  "description": "Starter kit for React, Webpack (with Hot Module Replacement), Typescript and Babel.",
  "keywords": [
    "react",
    "webpack",
    "typescript",
    "babel",
    "sass",
    "hmr",
    "starter",
    "boilerplate"
  ],
  "author": "Łukasz Garstecki, Tomasz Świerczewski",
  "license": "MIT",
  "scripts": {
    "build": "yarn run clean-dist && webpack -p --config=configs/webpack/prod.js",
    "clean-dist": "rm -f -r -d dist",
    "lint": "yarn run lint:ts && yarn run lint:sass",
    "lint:ts": "tslint './src/**/*.ts*' --format stylish --force",
    "lint:sass": "stylelint ./src/**/*.scss",
    "start": "yarn run start-dev",
    "start-dev": "webpack-dev-server --config=configs/webpack/dev.js",
    "dev": "webpack-dev-server --config=configs/webpack/dev.js",
    "start-prod": "yarn run build && node express.js",
    "test": "jest --watch --coverage --config=configs/jest.json"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.0",
    "@babel/core": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@types/jest": "^23.3.2",
    "@types/node": "10.12.12",
    "@types/react": "16.4.14",
    "@types/react-color": "2.14.0",
    "@types/react-dom": "16.0.11",
    "@types/react-redux": "6.0.9",
    "@types/redux-actions": "2.3.1",
    "@types/redux-logger": "3.0.6",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.2",
    "css-loader": "^1.0.0",
    "express": "^4.16.3",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^4.3.1",
    "jest": "^23.6.0",
    "node-sass": "^4.9.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.0",
    "stylelint": "^9.5.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-webpack-plugin": "^0.10.5",
    "tslint": "^5.11.0",
    "typescript": "3.0.3",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "webpack": "^4.19.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-middleware": "^3.3.0",
    "webpack-dev-server": "^3.1.8",
    "webpack-merge": "^4.1.4"
  },
  "dependencies": {
    "@material-ui/core": "3.5.1",
    "@material-ui/icons": "3.0.1",
    "cornerstone-core": "^2.2.6",
    "cornerstone-math": "^0.1.6",
    "cornerstone-tools": "^2.3.9",
    "cornerstone-wado-image-loader": "2.2.3",
    "cornerstone-web-image-loader": "^2.1.0",
    "hammerjs": "^2.0.8",
    "react": "16.6.3",
    "react-addons-test-utils": "15.6.2",
    "react-canvas-draw": "1.0.0",
    "react-color": "2.17.0",
    "react-dom": "16.6.3",
    "react-hot-loader": "4.3.12",
    "react-redux": "5.0.7",
    "redux": "3.7.2",
    "redux-actions": "2.6.4",
    "redux-logger": "3.0.6",
    "redux-thunk": "2.2.0"
  }
}
